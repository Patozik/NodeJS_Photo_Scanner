<%- include('partials/header'); -%>

  <div class="container pt-4" style="max-width: 700px">
    <div class="card">
      <div class="card-body text-center">
        <p>Lista zdjęć :</p>
            <% if(images.length > 0) { %>
                <% images.forEach(img => { %>
                    <img src="<%= img %>" style="max-width: 500px">
                <% }); %>
            <% } else { %>
                <p>Brak zdjęć na stronie</p>
            <% } %>
      </div>
    </div>
  </div>
  
<%- include('partials/footer'); -%>